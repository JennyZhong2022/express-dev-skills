<%-include('../partials/header') %>


  <h1>My skills</h1>

    <ul>
      <% skills.forEach((skill)=>{ %>
      <li>
        <form action="/skills/<%= skill.id %>?_method=DELETE"
          class="delete-form" method="POST">
          <button type="submit">X</button>
        </form>
        <a href="/skills/<%=skill.id %>"> <%= skill.tech %></a>
      
      </li>
      <% }) ;%>
     
    
  </ul>
  <br>
  <a href="/skills/new">Add New Skill</a>
  <%-include('../partials/footer') %>